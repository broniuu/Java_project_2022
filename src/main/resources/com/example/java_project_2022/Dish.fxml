<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400" prefWidth="800" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java_project_2022.DishController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="9.0" layoutY="14.0" prefHeight="400.0" prefWidth="800" spacing="30">
         <children>
            <Label fx:id="dishName" text="Label" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="dishImage" fitHeight="250" fitWidth="500" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="dishDescription" text="Label">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>

   </children>
</AnchorPane>
